[baseConfig]
pccBinary=@@@app@@@\pcc\bin\pcc.exe
pdbBinary=@@@app@@@\pcc\bin\pdb.exe
pcctagsBinary=@@@app@@@\pcc\bin\pcctags.exe
pccConfigFile=@@@app@@@\etc\pcc.conf
defaultProjectDir=@@@app@@@\projects

[Options]
tagsEnabled=1
debuggerEnabled=1
splashEnabled=1
saveWorkspace=1
backupsEnabled=1

